{
  "name": "sksadri/statamic-bard-text-direction",
  "description": "Statamic Bard text direction",
  "keywords": [
    "statamic",
    "bard",
    "text-direction"
  ],
  "autoload": {
    "psr-4": {
      "SKApps\\BardTextDirection\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SKApps\\BardTextDirection\\Tests\\": "tests"
    }
  },
  "homepage": "https://sks.tn",
  "license": "proprietary",
  "type": "statamic-addon",
  "require": {
    "php": "^7.4|^8.0"
  },
  "require-dev": {
    "orchestra/testbench": "4.*|5.*",
    "phpunit/phpunit": "^8.4|^9.0"
  },
  "scripts": {
    "test": "phpunit",
    "test-coverage": "phpunit --coverage-html coverage"
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "statamic": {
      "name": "Statamic Bard Text Direction",
      "description": "Text align command for bard"
    },
    "laravel": {
      "providers": [
        "SKApps\\BardTextDirection\\ServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
